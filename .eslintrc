{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "comma-dangle": 1,
      "quotes": [ 1, "single" ],
      "no-undef": 1,
      "global-strict": 0,
      "no-extra-semi": 1,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "no-unused-vars": 1,
      "no-trailing-spaces": [1, { "skipBlankLines": true }],
      "no-unreachable": 1,
      "no-alert": 1,
      "jsx-quotes": 1,
      "no-multi-spaces": 1,
      "no-extra-boolean-cast": 1,
      "no-redeclare": 1,
      "no-prototype-builtins": 1,
      "no-case-declarations": 1,
      "no-useless-escape": 1,
      "no-cond-assign": 1,
      "no-debugger": 1
    }
  }